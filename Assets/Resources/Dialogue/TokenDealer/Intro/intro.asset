  character: Token Dealer
  standard: 0
  identifier: 2
  lines:
  - speech: 'Hey, kid, Nice to meet ya''. '
    icon: {fileID: 21300000, guid: 486c19f5c3b99164093c3a6929f4706e, type: 3}
    textSpeed: 0
    stopMusic: 0
    stopSound: 0
    unskippable: 0
    voice: {fileID: 8300000, guid: ea83aa149ab69124481d4bd32ad3899f, type: 3}
  - speech: I'm the friendly neighba'hood <sprite index=2><color=#FF7300>TOKEN</color>
      dealer 'round here. How's it goin' pal?
    icon: {fileID: 21300000, guid: 486c19f5c3b99164093c3a6929f4706e, type: 3}
    textSpeed: 0
    stopMusic: 0
    stopSound: 0
    unskippable: 0
    voice: {fileID: 8300000, guid: ea83aa149ab69124481d4bd32ad3899f, type: 3}
  - speech: I heard ya' want some <sprite index=2><color=#FF7300>TOKENS</color>!
      Well, ya' came to the right place.
    icon: {fileID: 21300000, guid: 486c19f5c3b99164093c3a6929f4706e, type: 3}
    textSpeed: 0
    stopMusic: 0
    stopSound: 0
    unskippable: 0
    voice: {fileID: 8300000, guid: ea83aa149ab69124481d4bd32ad3899f, type: 3}
  - speech: My store here is full of dem things! But I don't give nothin' out for
      free, so here's my one-time exclusive deal.
    icon: {fileID: 21300000, guid: 486c19f5c3b99164093c3a6929f4706e, type: 3}
    textSpeed: 0
    stopMusic: 0
    stopSound: 0
    unskippable: 0
    voice: {fileID: 8300000, guid: ea83aa149ab69124481d4bd32ad3899f, type: 3}
  - speech: 'You do some little <color=#FF0000>errands</color> for me and I give
      you some <sprite index=2><color=#FF7300>TOKENS</color> in return! '
    icon: {fileID: 21300000, guid: 486c19f5c3b99164093c3a6929f4706e, type: 3}
    textSpeed: 0
    stopMusic: 0
    stopSound: 0
    unskippable: 0
    voice: {fileID: 8300000, guid: ea83aa149ab69124481d4bd32ad3899f, type: 3}
  branch:
    question: Ain't that sound fair, kid?
    icon: {fileID: 21300000, guid: 486c19f5c3b99164093c3a6929f4706e, type: 3}
    choices:
    - answer: Sure thing, boss!
      ensuingDialogue: {fileID: 11400000, guid: 8cb89b0ae2cc21a4d82217084b481119, type: 2}
    - answer: Why is your "store" made of cardboard?
      ensuingDialogue: {fileID: 11400000, guid: c60387d9df9eb494a928c8fbe34492f3, type: 2}
